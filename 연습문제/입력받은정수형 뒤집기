
public class Prac123 {
    public static void solution(int num) {
        int result = 0;
        while(num!=0) {
            result = result * 10 + num % 10;
            num /= 10;
        }
        System.out.println(result);
    }

    public static void main(String[] args) {
        // Test code
        solution(12345);
        solution(-12345);
        solution(100);
        solution(0);
    }
}
